<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <MenuBar id="menuBar" focusTraversable="true">
            <menus>
                <Menu text="Application">
                    <items>
                        <MenuItem text="Start" />
                        <MenuItem text="Load" />
                        <SeparatorMenuItem />
<!--                        <MenuItem text="Exit" onAction="#processExit" />-->
                    </items>
                </Menu>
                <Menu text="Details">
                    <items>
                        <MenuItem text="Enemy Ships" />
                        <MenuItem text="Player Shots" />
                        <MenuItem text="Enemy Shots" />
                    </items>
                </Menu>
                <Menu text="Help">
                    <items>
                        <MenuItem disable="true" text="Search" />
                        <MenuItem text="Online Manual" visible="false" />
                        <SeparatorMenuItem />
                        <MenuItem text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>

</BorderPane>
